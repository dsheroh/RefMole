<% IF numrecs == 0 %>
document.write ("<h3>The personal publication list is awaiting approval. Please consult later.</h3>");
document.write ("Further information: <a href=\"http://lup.lub.lu.se/\">http:lup.lub.lu.se/</a>");
<% STOP %>
<% END %>

document.write('<div class=citation-list>');
document.write ('<ul>');
<% FOREACH entry IN records -%>
document.write ('<li><%- entry.citation -%><a href=\"https://lup.lub.lu.se/record/<% entry.recordid %>\" title=\"Show complete bibliographic and access information\" target=\"_top\">PUB</a><% FOREACH rel_entry IN entry.constituent %><% IF rel_entry.label.0 %> | <a href=\"<% rel_entry.url.0 %>\" title=\"<% rel_entry.label.0 %>\" target=\"_top\">PDF</a><% END %><% END %><% FOREACH rel_entry IN entry.supplementaryMaterial %><% IF rel_entry.title %> | <a href=\"<% rel_entry.url %>\" title=\"<% rel_entry.title %>\" target=\"_top\">Supplementary Material</a><% END %><% END %><% FOREACH rel_entry IN entry.isPart %><% IF rel_entry.title %> | <a href=\"<% rel_entry.url %>\" title=\"<% rel_entry.title %>\" target=\"_top\">Related Material</a><% END %><% END %><% FOREACH rel_entry IN entry.host %><% IF rel_entry.doi.0 %>| <a href=\"http://dx.doi.org/<% rel_entry.doi.0 %>\" title=\"<% rel_entry.doi.0 %>\" target=\"_top\">DOI</a><% ELSE %><% IF rel_entry.url %>| <a href=\"<% rel_entry.url %>\" title=\"<% rel_entry.url %>\" target=\"_top\">URL</a><% END %><% END %><% IF rel_entry.medline.0 %> | <a href=\"http://www.ncbi.nlm.nih.gov/pubmed/<% rel_entry.medline.0 %>\" target=\"_top\">PubMed</a> | <a href=\"http://ukpmc.ac.uk/abstract/MED/<% rel_entry.medline.0 %>\">UK PMC</a><% END %><% IF rel_entry.arxiv.0 %> | <a href=\"http://arxiv.org/abs/<% rel_entry.arxiv.0 %>\" target=\"_top\">arXiv</a><% END %><% IF rel_entry.inspire.0 %> | <a href=\"http://inspirebeta.net/record/<% rel_entry.inspire.0 %>\" target=\"_top\">Inspire</a><% END %><% END %></li>');
<% END %>
document.write ('</ul>');

<% IF params.max_page > 1 %>
  document.write('<p><i><a href="cite?<% vars.clean_params %>">See more citations</a></i></p>');
<% END %>
document.write('</div>');

